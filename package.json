{
  "name": "webassembly-experiments",
  "version": "1.0.0",
  "repository": "git@github.com:rpellerin/webassembly-experiments.git",
  "author": "Romain Pellerin <romain.pellerin@inovia-team.com>",
  "license": "MIT",
  "dependencies": {
    "pushstate-server": "^2.2.1"
  },
  "scripts": {
    "build-c": "mkdir -p build; gcc c/main.c c/prime.c -o build/build-c",
    "build-c-o3": "mkdir -p build; gcc -O3 c/main.c c/prime.c -o build/build-c-o3",
    "c": " build/build-c",
    "c-o3": " build/build-c-o3",
    "build-asm": " build/prime_O3",
    "build-wasm": " build/prime_O3",
    "serve": "cp -f public/index.html public/plain-javascript.js build && ./node_modules/.bin/pushstate-server build 8080"
  }
}
